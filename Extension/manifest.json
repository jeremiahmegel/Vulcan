{
	"manifest_version": 2,
	"name": "Vulcan",
	"version": "1.0.4",
	"description": "Convert emoticons in Google Hangouts to their text equivalent.",
	"icons": {
		"16": "images/16.png",
		"48": "images/48.png",
		"128": "images/128.png"
	},
	"permissions": [
		"tabs",
		"*://talkgadget.google.com/*"
	],
	"content_scripts": [
		{
			"matches": [
				"*://talkgadget.google.com/*"
			],
			"all_frames": true,
			"css": [
				"css/content.css"
			]
		}
	]
}